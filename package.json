{
  "name": "@rpfeifer-soft/mui-controls",
  "version": "0.4.0",
  "private": false,
  "repository": {
    "url": "git://github.com/rpfeifer-soft/mui-controls.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "mobx": "^6.0.4",
    "mobx-react": "^7.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compile": "tsc --build ./src/package/tsconfig.json",
    "clean": "rimraf package/",
    "publish:prepare": "npm run clean && npm run compile && clear-package-json package.json > package/package.json && cp .npmignore package",
    "publish:try": "npm run publish:prepare && npm publish package --dry-run",
    "publish:patch": "npm version patch && npm run publish:prepare && npm publish package",
    "publish:minor": "npm version minor && npm run publish:prepare && npm publish package",
    "publish:major": "npm version major && npm run publish:prepare && npm publish package"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^12.19.9",
    "react-scripts": "4.0.1",
    "typescript": "^4.1.3",
    "web-vitals": "^0.2.4",
    "clean-publish": "^1.1.8"
  }
}
